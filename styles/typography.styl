  font-family 'LB'
  font-weight 600
  src url('/assets/fonts/lb/lb-b.woff2') format('woff2'),
      url('/assets/fonts/lb/lb-b.woff') format('woff'),
      url('/assets/fonts/lb/lb-b.ttf') format('truetype')


h1,h2,h3,h4,h5,h6
