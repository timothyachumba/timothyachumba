@font-face
  font-family 'AG'
  font-weight 600
  src url('/assets/fonts/ag/ag-b.woff2') format('woff2'),
      url('/assets/fonts/ag/ag-b.woff') format('woff'),
      url('/assets/fonts/ag/ag-b.ttf') format('truetype')

@font-face
  font-family 'LB'
  font-weight 400
